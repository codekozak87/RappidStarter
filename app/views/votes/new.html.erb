<h1>New Vote</h1>


<%= form_tag(votes_url, method: 'post') do %>


  <div>
    <%= label_tag :user_id %><br />
    <%= number_field_tag :user_id %>
  </div>

    <div>
    <%= label_tag :user_story_id %><br />
    <%= number_field_tag :user_story_id %>
  </div>

    <div>
    <%= label_tag :mock_up_id %><br />
    <%= number_field_tag :mock_up_id %>
  </div>

    <div>
    <%= label_tag :layout_id %><br />
    <%= number_field_tag :layout_id %>
  </div>

  
  <div>
    <%= submit_tag "Create Vote" %>
  </div>
<% end %>
